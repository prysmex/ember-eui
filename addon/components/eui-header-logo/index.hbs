<a
  class="euiHeaderLogo"
  href={{@href}}
  target={{@target}}
  ...attributes
>
  <EuiIcon
    aria-label={{@iconTitle}}
    class="euiHeaderLogo__icon"
    @size="l"
    @type={{arg-or-default @iconType "logoElastic"}}
  />
  {{#if (has-block)}}
    <span class="euiHeaderLogo__text">
      {{yield}}
    </span>
  {{/if}}
</a>
