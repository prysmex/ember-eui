<input
  type="range"
  id={{@id}}
  name={{@name}}
  class={{class-names
    "euiRangeSlider"
    (if @showTicks "euiRangeSlider--hasTicks")
    (if @hasFocus "euiRangeSlider--hasFocus")
    (if @showRange "euiRangeSlider--hasRange")
    (if @compressed "euiRangeSlider--compressed")
  }}
  min={{@min}}
  max={{@max}}
  step={{@step}}
  value={{@value}}
  disabled={{@disabled}}
  ...attributes
/>