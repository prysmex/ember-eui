{{#if (eq @tagName "div")}}
  <div
    class={{class-names componentName="EuiTextColor" color=(arg-or-default @color "default")}}
    ...attributes
  >
    {{yield}}
  </div>
{{else}}
  <span
    class={{class-names componentName="EuiTextColor" color=(arg-or-default @color "default")}}
    ...attributes
  >
    {{yield}}
  </span>
{{/if}}