{
  "name": "@ember-eui/core",
  "version": "4.0.4",
  "description": "Ember Components for Elastic UI",
  "keywords": [
    "ember-addon",
    "elastic",
    "design",
    "ember",
    "eui",
    "ember-cli-netlify-plugin"
  ],
  "repository": "https://github.com/prysmex/ember-eui",
  "license": "MIT",
  "author": "Alberto Cantu <betocantu93@gmail.com> (https://betocantu93.com)",
  "contributors:": [
    "David Martinez",
    "Mario Trujillo",
    "Victor Aguilar",
    "Diego Zubieta",
    "Gustavo Rodriguez"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{json,css,scss,md,ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@elastic/eui": "41.4.0",
    "@ember/render-modifiers": "^2.0.4",
    "@html-next/vertical-collection": "html-next/vertical-collection#v3.0.0-0",
    "@types/lodash-es": "^4.17.4",
    "chroma-js": "^2.1.0",
    "ember-auto-import": "^2.2.4",
    "ember-cached-decorator-polyfill": "^0.1.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "4.2.1",
    "ember-composable-helpers": "^4.4.1",
    "ember-element-helper": "^0.5.5",
    "ember-in-element-polyfill": "^1.0.0",
    "ember-keyboard": "^7.0.1",
    "ember-math-helpers": "^2.15.0",
    "ember-modifier": "^3.1.0",
    "ember-power-select": "^5.0.2",
    "ember-set-body-class": "^1.0.2",
    "ember-set-helper": "^2.0.1",
    "ember-style-modifier": "^0.7.0",
    "ember-svg-jar": "^2.3.4",
    "ember-truth-helpers": "^3.0.0",
    "highlight.js": "^9.18.5",
    "lodash-es": "^4.17.21",
    "mdast-util-to-hast": "^10.0.0",
    "rehype-raw": "^5.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-breaks": "^3.0.2",
    "remark-emoji": "^2.1.0",
    "remark-highlight.js": "^6.0.0",
    "remark-parse": "^8.0.3",
    "remark-rehype": "^8.0.0",
    "resolve": "^1.13.1",
    "tabbable": "^5.1.5",
    "unified": "^9.2.0",
    "vfile": "^4.2.0"
  },
  "peerDependencies": {
    "ember-focus-trap": "^1.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.0.0",
    "@embroider/util": "^1.2.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^5.0.0",
    "@types/ember-resolver": "^5.0.11",
    "@types/ember__application": "^4.0.0",
    "@types/ember__array": "^4.0.1",
    "@types/ember__component": "^4.0.4",
    "@types/ember__controller": "^4.0.0",
    "@types/ember__debug": "^4.0.0",
    "@types/ember__engine": "^4.0.0",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.0",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.3",
    "@types/ember__runloop": "^4.0.0",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.0.9",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.1.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-focus-trap": "^1.0.1",
    "ember-get-config": "~1.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "lodash-es": "^4.17.21",
    "mdast-util-to-hast": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.65.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBluerprint": "ember-eui-core"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
