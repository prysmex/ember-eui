<EuiPage>
  <EuiPageSideBar>
    <EuiSideNav
      @mobileTitle="Elastic UI"
      @isOpenMobile={{this.isOpenMobile}}
      @toggleOpenOnMobile={{set this.isOpenMobile}}
      @items={{this.sideNavItems}}
      @selectedItem={{this.router.currentRouteName}}
    />
  </EuiPageSideBar>
  <EuiPageBody @tag="div">
    {{outlet}}
  </EuiPageBody>
</EuiPage>